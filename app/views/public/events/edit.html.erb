<div style="height: 120px"></div>
<div class="event-form container">
  <div class="row justify-content-center">
    <div class="col-md-8">
      <h1 class="event-form__title">イベント修正ページ</h1>
      <%= form_with model: @event, local: true, url: event_path(@event) do |f| %>
        <%= render partial: 'public/events/form', locals: { f: f, isEdit: true } %>
      <% end %>
    </div>
  </div>
</div>
<div style="height: 120px;"></div>