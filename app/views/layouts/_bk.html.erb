 
<div>
  <div id="navbarNavDropdown">
    <ul>
      <li>
        <div>
          <%= form_with url: search_path, method: :get do |f| %>
            <%= f.text_field :content %>
            <%= f.select :model, options_for_select({"全てから探す" => nil, "User" => "user", "Post" => "post", "Event" => "event", "Hashtag" => "hashtag"}) %>
            <%= f.select :method,options_for_select({"完全一致"=>"perfect","部分一致"=>"partial"})%>
            <%= f.submit'検索' %>
          <% end %>
        </div>
      </li>
      <li>
        <%= link_to posts_path, class: '' do %>
          Topic
        <% end %>
      </li>
      <li>
        <%= link_to events_path, class: '' do %>
          Event
        <% end %>
      </li>
      <% if user_signed_in? %>
        <li>
          <%= link_to my_page_users_path, class: '' do %>
            Mypage
          <% end %>
        </li>
        <li>
          <%= link_to destroy_user_session_path, method: :delete, class: '' do %>
            Logout
          <% end %>
        </li>
      <% elsif admin_signed_in? %>
        <li>
          <%= link_to admin_root_path, class: '' do %>
            Users
          <% end %>
        </li>
        <li>
          <%= link_to destroy_admin_session_path, method: :delete, class: '' do %>
            Logout
          <% end %>
        </li>
      <% else %>
        <li>
          <%= link_to new_user_registration_path, class: '' do %>
            Signup
          <% end %>
        </li>
        <li>
          <%= link_to new_user_session_path, class: '' do %>
            Login
          <% end %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
